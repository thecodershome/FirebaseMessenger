<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="g20.homework8">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".Application"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".DispatchActivity"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="@string/loginLabel" />
        <activity
            android:name=".MainActivity"
            android:label="@string/stayintouchLabel" />
        <activity
            android:name=".SignupActivity"
            android:label="@string/signupLabel" />
        <activity
            android:name=".ProfileDetailsActivity"
            android:label="@string/editProfileLabel" />
        <activity
            android:name=".MessageActivity"
            android:label="@string/messagesLabel" />
        <activity
            android:name=".DisplayContactActivity"
            android:label="@string/viewContact_Label" />
    </application>

</manifest>